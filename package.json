{
  "name": "vue-tabled",
  "description": "A Vue.js table component",
  "author": "Victor Pereira",
  "license": "MIT",
  "private": false,
  "version": "3.0.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovictorpereira/vue-tabled.git"
  },
  "homepage": "https://github.com/ovictorpereira/vue-tabled",
  "keywords": [
    "vue",
    "vue3",
    "table",
    "pagination",
    "bootstrap-table",
    "library",
    "vue-tabled"
  ],
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/vue-tabled.umd.cjs",
  "module": "./dist/vue-tabled.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-tabled.js",
      "require": "./dist/vue-tabled.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "path": "^0.12.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  }
}
